          "rawSql": "WITH latest AS (\n  SELECT DISTINCT ON (exchange) \n    exchange,\n    last_price,\n    bid_price,\n    ask_price,\n    volume_24h,\n    timestamp\n  FROM market_data\n  WHERE symbol = '$symbol'\n    AND timestamp > NOW() - INTERVAL '5 minutes'\n  ORDER BY exchange, timestamp DESC\n)\nSELECT \n  exchange AS \"Exchange\",\n  ROUND(last_price::numeric, 6) AS \"Last Price\",\n  ROUND(bid_price::numeric, 6) AS \"Bid\",\n  ROUND(ask_price::numeric, 6) AS \"Ask\",\n  ROUND((ask_price - bid_price)::numeric, 6) AS \"Spread\",\n  ROUND(((ask_price - bid_price) / NULLIF(bid_price, 0) * 10000)::numeric, 2) AS \"Spread (bps)\",\n  ROUND(volume_24h::numeric, 2) AS \"24h Volume\",\n  timestamp AS \"Last Update\"\nFROM latest\nORDER BY last_price DESC",
