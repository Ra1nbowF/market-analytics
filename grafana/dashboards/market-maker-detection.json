      "targets": [
        {
          "datasource": "grafana-postgresql-datasource",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH latest_book AS (\n  SELECT \n    bids::jsonb as bids,\n    asks::jsonb as asks,\n    timestamp\n  FROM orderbook_snapshots\n  WHERE symbol = '$symbol'\n    AND exchange = '$exchange_filter'\n  ORDER BY timestamp DESC\n  LIMIT 1\n),\nbid_volumes AS (\n  SELECT \n    (bid_data->>0)::numeric as bid_price,\n    ROUND((bid_data->>1)::numeric, 4) as volume\n  FROM latest_book,\n    jsonb_array_elements(bids) as bid_data\n  LIMIT 10\n),\nask_volumes AS (\n  SELECT \n    (ask_data->>0)::numeric as ask_price,\n    ROUND((ask_data->>1)::numeric, 4) as volume\n  FROM latest_book,\n    jsonb_array_elements(asks) as ask_data\n  LIMIT 10\n)\nSELECT \n  bv.volume as \"Order Size\",\n  bv.bid_price as \"Bid Price\",\n  av.ask_price as \"Ask Price\",\n  av.ask_price - bv.bid_price as \"Spread\",\n  'SYMMETRIC' as \"Type\"\nFROM bid_volumes bv\nJOIN ask_volumes av ON bv.volume = av.volume\nORDER BY bv.volume DESC",
          "refId": "A"
        }
      ],
      "title": "Symmetric Orders Detection",
      "type": "table"
    },
