      "targets": [
        {
          "datasource": "grafana-postgresql-datasource",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH patterns AS (\n  SELECT \n    'Round Number Orders' as pattern_type,\n    COUNT(CASE \n      WHEN (bids::jsonb->0->>0)::numeric % 100 = 0 \n      OR (asks::jsonb->0->>0)::numeric % 100 = 0 \n      THEN 1 \n    END) as count\n  FROM orderbook_snapshots\n  WHERE symbol = '$symbol' \n    AND exchange = '$exchange_filter'\n    AND timestamp > NOW() - INTERVAL '1 hour'\n  \n  UNION ALL\n  \n  SELECT \n    'Large Orders (Walls)' as pattern_type,\n    COUNT(CASE \n      WHEN (bids::jsonb->0->>1)::numeric > 5 \n      OR (asks::jsonb->0->>1)::numeric > 5\n      THEN 1 \n    END) as count\n  FROM orderbook_snapshots\n  WHERE symbol = '$symbol' \n    AND exchange = '$exchange_filter'\n    AND timestamp > NOW() - INTERVAL '1 hour'\n  \n  UNION ALL\n  \n  SELECT \n    'Rapid Changes' as pattern_type,\n    COUNT(*) as count\n  FROM (\n    SELECT \n      ob.*,\n      LAG((ob.bids::jsonb->0->>0)::numeric) OVER (ORDER BY ob.timestamp) as prev_bid\n    FROM orderbook_snapshots ob\n    WHERE ob.symbol = '$symbol' \n      AND ob.exchange = '$exchange_filter'\n      AND ob.timestamp > NOW() - INTERVAL '1 hour'\n  ) t\n  WHERE ABS((t.bids::jsonb->0->>0)::numeric - t.prev_bid) > 10\n)\nSELECT * FROM patterns",
          "refId": "A"
        }
      ],
      "title": "MM Pattern Detection",
      "type": "barchart"
    },
