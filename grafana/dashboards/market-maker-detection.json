        {
          "datasource": "grafana-postgresql-datasource",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "-- Market Maker Detection Score\n-- Based on order book patterns\nWITH pattern_analysis AS (\n  SELECT \n    exchange,\n    COUNT(CASE WHEN (bids::jsonb->0->>0)::numeric % 100 = 0 THEN 1 END) as round_orders,\n    COUNT(*) as total_snapshots\n  FROM orderbook_snapshots\n  WHERE symbol = '$symbol'\n    AND timestamp > NOW() - INTERVAL '30 minutes'\n  GROUP BY exchange\n)\nSELECT \n  CASE \n    WHEN round_orders > 5 THEN 80\n    WHEN round_orders > 2 THEN 50\n    ELSE 20\n  END as mm_probability\nFROM pattern_analysis\nWHERE exchange = '$exchange_filter'",
          "refId": "A"
        }
      ],
      "title": "MM Probability Score",
      "type": "gauge"
    },
